package br.com.fundatec.main;

public class ContaDeEstudante{

    private int milhas;

    public void deposita(double valor) {
        //super.deposita(valor);
        this.milhas += (int)valor;
    }

    public int getMilhas() {
        return milhas;
    }

    public void rende() {
        throw new RuntimeException("Não pode render");
    }
}
